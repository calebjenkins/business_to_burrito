{
"manifest_version": 2,
    "name": "business_to_burrito",
    "description": "A Chrome extension which replaces the word 'business' with 'burrito'",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}
