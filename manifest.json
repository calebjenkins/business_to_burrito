{
"manifest_version": 2,
    "name": "Business to Burrito",
    "description": "A Chrome extension which replaces the word 'business' with 'burrito'.",
    "version": "0.4",
    "author": "Kerri Miller",
    "homepage_url": "http://github.com/kerrizor/business_to_burrito",
    "short_name": "Business to Burrito",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}
